<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.activity.LaunchActivity">

    <ImageView
        android:id="@+id/imageView_launch"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:src="@drawable/logo_a"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.32999998"
        android:contentDescription="@string/image_is_blank" />

    <ImageView
        android:id="@+id/imageView_launch_yanfa"
        android:layout_width="120dp"
        android:layout_height="40dp"
        android:src="@drawable/yanfa_tittle"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/imageView_launch"/>


</androidx.constraintlayout.widget.ConstraintLayout>